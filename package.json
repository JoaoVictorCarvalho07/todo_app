{
  "name": "todo_app",
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently \"npm run dev --prefix backend\" \"npm start --prefix frontend\"",
    "install:all": "concurrently \"npm install --prefix backend\" \"npm install --prefix frontend\"",
    "setup": "npm install && npm run install:all"
  },
  "dependencies": {
    "concurrently": "^9.0.1"
  }
}
